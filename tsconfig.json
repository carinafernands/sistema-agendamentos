{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "rootDir": "src",
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "typeRoots": ["./node_modules/@types", "./types"],
      "types": ["node", "express"],
    },
    "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
  